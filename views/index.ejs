        <link href="/stylesheets/style.css" rel="stylesheet" />
        <script src="/javascripts/script.js" type="text/javascript"></script>

        <!-- import Google Maps Script -->
        <script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAK8MrWHM4riyD4HJ6M5pabVAs09scuLBU&libraries=place?callback=initMap">
