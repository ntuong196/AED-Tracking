        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
        <!-- <link rel="icon" href="/images/favicon.ico" /> -->
        <title><%= title %></title>

        
    </head>
    <body>
        <%- include('./partial/header') %>
        
        <div class="image-container" >
